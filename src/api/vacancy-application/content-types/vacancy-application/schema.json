{
  "kind": "collectionType",
  "collectionName": "vacancy_applications",
  "info": {
    "singularName": "vacancy-application",
    "pluralName": "vacancy-applications",
    "displayName": "VacancyApplication"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "vacancy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vacancy.vacancy",
      "inversedBy": "applications"
    },
    "full_name": {
      "type": "string",
      "required": true
    },
    "birth_date": {
      "type": "date"
    },
    "sex": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "other"
      ]
    },
    "place_of_birth": {
      "type": "string"
    },
    "nationality": {
      "type": "string",
      "required": true
    },
    "education_background": {
      "type": "text"
    },
    "short_courses": {
      "type": "text"
    },
    "work_history": {
      "type": "text"
    },
    "current_address": {
      "type": "text"
    },
    "sub_city": {
      "type": "string"
    },
    "woreda": {
      "type": "string"
    },
    "phone_number": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "reference_1": {
      "type": "string"
    },
    "reference_2": {
      "type": "string"
    },
    "reference_3": {
      "type": "string"
    }
  }
}
